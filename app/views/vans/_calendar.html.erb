
<%if @check_orders == 0%>
<div>
      <%= month_calendar(attribute: :start_date, end_attribute: :end_date) do |date, orders| %>
      <div class="day">
          <%= date.day %>
        </div>
      <% end %>
</div>
<%else%>
<div>
      <%= month_calendar(events:@orders, attribute: :start_date, end_attribute: :end_date) do |date, orders| %>
      <div class="day">
          <%= date.day %>
        </div>
         <%orders.each do |order|%>
        <%end%>
      <% end %>
</div>
<%end%>
